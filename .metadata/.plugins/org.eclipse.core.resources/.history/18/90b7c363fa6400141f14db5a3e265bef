public class AOR implements Serializable{

	
	private String name;
	private String host;
	private String AOR;
	public AOR(String name,String host){
		
		this.name=name;
		this.host=host;
		this.AOR="sip: "+name+"@"+host;
		
		
	}
	
	
	
	
	
	
	
	
}
